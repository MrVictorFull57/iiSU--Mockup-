        <label language="zh_TW">預設無時鐘</label>
		<selectable>true</selectable>
	</variant>
	<variant name="gamelist-carousel-neither">
		<label language="en_US">Default without Either</label>
        <label language="ca_ES">Per Defecte Sense Cap Dels Dos</label>
        <label language="de_DE">Standardmäßig ohne</label>
