        <label language="nl_NL">Standaard Zonder Klok</label>
        <label language="pl_PL">Domyślnie Bez Zegara</label>
        <label language="pt_BR">Padrão Sem Relógio</label>
        <label language="ro_RO">Implicit Fără Ceas</label>
